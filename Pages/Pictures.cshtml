@page
@model WebTest2026.Pages.PicturesModel
@{
	ViewData["Title"] = "Pictures";
}

<div class="text-center">
	<h1 class="display-6">Pictures from the Yorkshire Dales</h1>
	<img src="~/images/litton.jpg" height="500" alt="Litton" />
	<h2>Litton Village</h2>
	<p>A small village in Littondale, near Grassington.</p>
	<img src="~/images/fell.jpg" height="500" alt="Fell"/>
	<h2>Fell Walk</h2>
	<p>A walk up to the top of a fell above Littondale.</p>
	<img src="~/images/pheasant.jpg" height="500" alt="Pheasant" />
	<h2>Pheasant</h2>
	<p>A pheasant spotted on the fell walk.</p>
	<img src="~/images/cows.jpg" height="500" alt="Cows"/>
	<h2>Cows</h2>
	<p>Cows on the fell above Littondale.</p>
</div>
